# GROQ API Configuration
GROQ_API_KEY=your_groq_api_key_here
GROQ_MODEL=llama3.1-8b-instant

# Web Server Configuration
WEB_HOST=localhost
WEB_PORT=8000

# BlockAGI Configuration
BLOCKAGI_AGENT_ROLE=Research Assistant
BLOCKAGI_ITERATION_COUNT=3

# Research Objectives (optional - can be set via command line)
BLOCKAGI_OBJECTIVE_1=Bitcoin adoption trends
BLOCKAGI_OBJECTIVE_2=DeFi security analysis
BLOCKAGI_OBJECTIVE_3=Blockchain scalability solutions

# Optional: Google Search API (for additional search capabilities)
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CSE_ID=your_custom_search_engine_id_here

# Optional: OpenAI API (if you want to keep both options)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo-16k
